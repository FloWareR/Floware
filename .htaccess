<Files "*.php">
  Require ip 127.0.0.1
</Files>

<Files "index.php">
    Require all granted
</Files>

RewriteEngine On
RewriteBase /Floware

RewriteCond %{THE_REQUEST} /public/([^\s?]*) [NC]
RewriteEngine On
#RewriteCond %{HTTPS} off
#RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R,L]
RewriteRule ^ %1 [L,NE,R=302]
RewriteRule ^(.*)$ public/index.php?$1 [L,QSA]